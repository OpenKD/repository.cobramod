msgid "     [COLOR orangered]>>> DELETE Database completely = CLICK HERE >>>[/COLOR]"
msgstr "     [COLOR orangered]>>> Database komplett LÖSCHEN = HIER KLICKEN >>>[/COLOR]"

msgctxt "#30112"
msgid "     Delete selected titles in Crontab and on the system ?"
msgstr "     Ausgewählte Titel im Crontab und auf dem System löschen ?"

